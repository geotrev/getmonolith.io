{
  "name": "undernet",
  "version": "5.3.0",
  "homepage": "https://undernet.io",
  "description": "A modular, configuration-first front-end framework. No strings.",
  "author": "George Treviranus",
  "license": "MIT",
  "private": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "sass": "src/scss/undernet.scss",
  "style": "dist/undernet.css",
  "scripts": {
    "postinstall": "npm run build:dist",
    "watch": "webpack-dev-server --config webpack.dev.js --progress",
    "build": "npm run js:build && webpack --config webpack.prod.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "npm run scss:lint && npm run js:lint",
    "scss:lint": "stylelint '**/*.scss'",
    "scss:lint:check": "stylelint-config-prettier-check",
    "js:lint": "eslint . --ext .js -c .eslintrc.js --ignore-path .eslintignore",
    "build:dist": "npm run js:build && npm run js:build:rollup && npm run css:build && npm run css:minify",
    "build:dist:release": "npm run clean && npm run scss:prepublish && npm run css:prepublish && npm run js:prepublish && npm run create-hashes",
    "prettify": "npm run js:prettify && npm run scss:prettify",
    "clean": "rm -rf dist/ lib/ && mkdir dist/ lib/",
    "js:build": "npm run js:build:cjs && npm run js:build:esm",
    "js:build:cjs": "BABEL_ENV=cjs babel src/js/ -d lib/cjs/ --ignore 'src/js/index.bundle.js','src/js/**/*.spec.js' --no-comments -s",
    "js:build:esm": "BABEL_ENV=esm babel src/js/ -d lib/esm/ --ignore 'src/js/index.bundle.js','src/js/**/*.spec.js' --no-comments -s",
    "js:build:rollup": "BABEL_ENV=cjs rollup -c rollup.config.js",
    "js:watch": "BABEL_ENV=esm babel src/js/ -d lib/esm/ --watch --ignore 'src/js/index.bundle.js' --no-comments -s",
    "js:prettify": "prettier --write 'src/js/**/*.js'",
    "js:modules:zip": "cd src/js/ && zip -x '/*__tests__/*' -r ../../dist/undernet.modules.js.zip .",
    "js:zip": "cd dist/ && zip -r undernet.js.zip . -x *.css *.zip *.css.map",
    "js:prepublish": "npm run js:build && npm run js:build:rollup && npm run js:modules:zip && npm run js:zip",
    "css:minify": "node-sass --source-map true --output-style compressed src/scss/undernet.scss dist/undernet.min.css",
    "css:build": "node-sass --source-map true src/scss/undernet.scss dist/undernet.css",
    "css:package": "npm run css:build && npm run css:minify",
    "css:prepublish": "npm run css:package && cd dist/ && zip -r undernet.css.zip . -x *.js *.zip *.js.map",
    "scss:prettify": "prettier --write 'src/scss/**/*.scss'",
    "scss:prepublish": "cd src/scss/ && zip -r ../../dist/undernet.scss.zip .",
    "update-version": "node bin/update-version.js",
    "create-hashes": "node bin/create-hashes.js",
    "preversion": "npm run js:build && npm test",
    "postversion": "npm run update-version && npm run build:dist:release && git add -A && git commit --amend"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.0",
    "core-js": "^3.1.4",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.2",
    "eslint-plugin-react": "^7.14.2",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jshashes": "^1.0.7",
    "lodash": "^4.17.11",
    "lottie-web": "^5.5.5",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.6",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^5.0.0",
    "react-router-last-location": "^2.0.0-beta.3",
    "react-spinners": "^0.5.1",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^11.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-scss": "^3.11.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "scss",
    "javascript",
    "js",
    "css",
    "frontend",
    "framework",
    "web"
  ],
  "files": [
    "dist/*.{css,js,map}",
    "lib/{esm,cjs}/*.{js,map}",
    "src/scss/**/*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geotrev/undernet.git"
  },
  "bugs": {
    "url": "https://github.com/geotrev/undernet/issues"
  }
}
