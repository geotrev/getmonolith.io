{
  "name": "undernet",
  "version": "2.2.0",
  "homepage": "https://undernet.io",
  "description": "Undernet is a Front-End Framework for Modern Web Development",
  "main": "js/dist/undernet.js",
  "author": "George Treviranus",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.1.0"
  },
  "keywords": [
    "scss",
    "javascript",
    "js",
    "css",
    "front-end",
    "framework"
  ],
  "scripts": {
    "setup": "rm -rf undernet.wiki/ && git clone https://github.com/geotrev/undernet.wiki.git && npm install",
    "test": "nyc mocha -r @babel/register -r config/testHelpers.js --recursive src/**/*.spec.js",
    "test:w": "mocha --watch -r @babel/register -r config/testHelpers.js --recursive src/**/*.spec.js",
    "build": "BABEL_ENV=production webpack --config config/webpack.prod.js --progress",
    "watch": "npm run js:build && webpack-dev-server --config config/webpack.dev.js --progress --open",
    "lint": "prettier --write 'src/**/*.js' && prettier --write 'src/**/*.scss'",
    "build:development": "npm run build:lint && npm run js:build && npm run css:build && npm run css:minify",
    "build:lint": "npm run js:lint && npm run scss:lint",
    "build:release": "npm run clean && npm run build:lint && npm run scss:prepublish && npm run css:prepublish && npm run js:prepublish",
    "clean": "rm -rf dist/ && mkdir dist/",
    "js:build": "babel js/src/ --out-dir js/dist/",
    "js:merge": "BABEL_ENV=rollup rollup -c config/rollup.config.js",
    "js:minify": "BABEL_ENV=rollup rollup -c config/rollup.config.prod.js",
    "js:watch": "babel js/src/ --watch --out-dir js/dist/",
    "js:modules:zip": "cd js/dist/ && zip -r ../../dist/undernet.modules.js.zip .",
    "js:zip": "cd dist/ && zip -r undernet.js.zip . -x *.css *.zip *.css.map",
    "js:prepublish": "npm run js:build && npm run js:merge && npm run js:modules:zip && npm run js:minify && npm run js:zip",
    "js:lint": "prettier --write 'js/src/**/*.js'",
    "css:minify": "sass scss/undernet.scss:dist/undernet.min.css --style compressed",
    "css:build": "sass scss/undernet.scss:dist/undernet.css",
    "css:package": "npm run css:build && npm run css:minify",
    "scss:lint": "prettier --write 'scss/**/*.scss'",
    "scss:prepublish": "cd scss/ && zip -r ../dist/undernet.scss.zip .",
    "css:prepublish": "npm run css:package && cd dist/ && zip -r undernet.css.zip . -x *.js *.zip *.js.map",
    "heroku:default": "heroku git:remote -a"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-loader": "^8.0.1",
    "babel-plugin-webpack-aliases": "^1.1.3",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-spies": "^1.0.0",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.2",
    "express-static-gzip": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^3.1.0",
    "jsdom": "^12.0.0",
    "markdown-loader": "^4.0.0",
    "mocha": "^5.0.5",
    "node-sass": "^4.9.3",
    "nyc": "^13.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.12.1",
    "prismjs": "^1.14.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.8",
    "react-markdown": "^3.1.4",
    "react-router-dom": "^4.2.2",
    "resolve-url-loader": "^2.3.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^5.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglify-js": "^3.3.27",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geotrev/undernet.git"
  }
}
